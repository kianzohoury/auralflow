{
    "model_params": {
        "separation_task": "mask",
        "model_type": "SpectrogramLSTM",
        "model_name": "my_separation_model",
        "normalize_input": true,
        "mask_activation": "hardtanh",
        "hidden_size": 16,
        "dropout_p": 0,
        "leak_factor": 0.2
    },
    "dataset_params": {
        "dataset_path": "/content/drive/MyDrive/MUSDB18/wav",
        "targets": ["vocals"],
        "max_num_tracks": 20,
        "max_num_samples": 1e3,
        "num_channels": 1,
        "sample_length": 2,
        "sample_rate": 44100,
        "num_fft": 1024,
        "window_size": 1024,
        "hop_length": 512,
        "power_spectrum": false,
        "learn_filterbanks": false,
        "val_split": 0.2
    },
    "training_params": {
        "training_mode": true,
        "max_epochs": 100,
        "batch_size": 64,
        "lr": 0.005,
        "criterion": "component_loss",
        "alpha_constant": 0.5,
        "beta_constant": 0,
        "num_workers": 8,
        "pin_memory": true,
        "persistent_workers": true,
        "pre_fetch": 4,
        "stop_patience": 10,
        "max_lr_reductions": 5,
        "checkpoint_path": "checkpoints",
        "last_epoch": -1,
        "global_step": -1
    },
    "visualizer_params": {
        "tensorboard": true,
        "save_images": false,
        "save_frequency": 5,
        "log_gradient": true,
        "num_images_view": 1,
        "num_audio_listen": 1,
        "logs_path": "runs"
    }
}
